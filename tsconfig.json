{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "preserve",
    "sourceMap": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "types": ["jest", "node"],
  },
  "include": ["src", "config/**/*",],

}
