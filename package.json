{
  "private": true,
  "name": "webpack-typescript-react-starter",
  "version": "1.0.0",
  "repository": "https://github.com/dudiharush/react-webpack-babel-typescript-starter.git",
  "author": "David Harush <davidharush@gmail.com>",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc",
    "check-packages": "npmPkgJsonLint -c ./config/packagejsonlint.config.js .",
    "build:prod": "webpack --config=config/webpack/webpack.config.prod.ts",
    "build:analyze": "webpack --config=config/webpack/webpack.config.analyze.ts",
    "build": "run-s -s clean-dist build:prod",
    "clean-dist": "rimraf dist/*",
    "start": "webpack serve --config=config/webpack/webpack.config.dev.ts",
    "test:jest:watch": "jest --watch --config=./config/jest.config.js",
    "test:jest": "jest -i --coverage --config=./config/jest.config.js",
    "test:staged": "jest --config=./config/jest.config-staged.js",
    "lint:fix": "eslint src/ --ext .tsx,.ts,.js --fix",
    "lint:fix:staged": "eslint --fix",
    "lint:fix-dev": "eslint -c ./dev.eslintrc.js src/ --ext .tsx,.ts,.js --fix",
    "lint:fix-dev:staged": "eslint -c ./dev.eslintrc.js --fix",
    "typecheck": "run-p typecheck-webapp typecheck-cypress",
    "typecheck-webapp": "tsc",
    "typecheck-cypress": "tsc -p cypress/tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:browser": "npm start && cypress run --browser chrome",
    "start-server": "webpack serve --config=config/webpack/webpack.config.dev.ts --env test",
    "cy:start-server-and-test": "npx start-server-and-test start-server http-get://localhost:8080 cy:run",
    "test:cypress:dev": "npm-run-all --parallel --race start cy:open",
    "test:unit:watch": "react-scripts test --env=jsdom",
    "test": "run-s --silent test:jest cy:start-server-and-test",
    "check-lock": "node lockCheck.js"
  },
  "jest": {
    "projects": [
      "<rootDir>/config/jest.config.ts"
    ]
  },
  "dependencies": {
    "npm-run-all": "4.1.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-info": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/cypress": "7.0.1",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@types/css-minimizer-webpack-plugin": "1.1.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "24.9.1",
    "@types/mini-css-extract-plugin": "1.2.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.48",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "16.9.2",
    "@types/terser-webpack-plugin": "5.0.2",
    "@types/webpack": "4.41.25",
    "@types/webpack-bundle-analyzer": "3.9.0",
    "@types/webpack-dev-server": "3.11.1",
    "@types/webpack-merge": "4.1.5",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.2.1",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "6.0.3",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.1.5",
    "cypress": "5.5.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "html-webpack-plugin": "4.5.0",
    "husky": "3.1.0",
    "install": "0.13.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "lock-verify": "2.2.1",
    "mini-css-extract-plugin": "1.3.1",
    "npm": "6.14.8",
    "npm-package-json-lint": "5.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.7.2",
    "react-refresh": "0.8.3",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.11.0",
    "style-loader": "2.0.0",
    "ts-node": "8.10.2",
    "typescript": "4.0.5",
    "webpack": "5.6.0",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix:staged",
      "npm run lint:fix-dev:staged",
      "git add",
      "npm run test:staged"
    ],
    "package*.json": [
      "npm run check-lock"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run typecheck && npm run check-packages"
    }
  }
}
