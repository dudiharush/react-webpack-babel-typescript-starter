    "@types/react-dom": "^16.9.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "serve": "^11.2.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
