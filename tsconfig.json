{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "preserve",
    "sourceMap": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "types": ["jest","cypress", "@testing-library/cypress"],
  },
  "include": ["src", "config/**/*", "cypress/integration/*.ts"],

}
